---
import { GoogleAnalytics } from '@astrolib/analytics';
import { ANALYTICS } from 'site-config';

const isProd = import.meta.env.PROD;
---

{
  isProd && ANALYTICS?.vendors?.googleAnalytics?.id ? (
    <GoogleAnalytics
      id={ANALYTICS.vendors.googleAnalytics.id}
      partytown={ANALYTICS.vendors.googleAnalytics.partytown}
    />
  ) : null
}
